name'\n                            value={values.username}\n                            error={errors.username ? true : false}\n                            onChange={onChange}\n                        />\n                        <Form.Input\n                            fluid\n                            icon='lock'\n                            iconPosition='left'\n                            placeholder='Password'\n                            type='password'\n                            name='password'\n                            value={values.password}\n                            error={errors.password ? true : false}\n                            onChange={onChange}\n                        />\n                        <Button color='teal' fluid type='submit' primary>\n                            Login\n                        </Button>\n                    </Form>\n                    <Divider />\n                    <div>\n                        <Label style={{ fontSize: '15px' }}>\n                            Not Register?\n                        </Label>\n                        <Icon name='hand point right' size='large' />\n                        <Label style={{ fontSize: '10px' }} />\n                        <Button\n                            icon\n                            labelPosition='left'\n                            as={Link}\n                            to='/register'\n                            color='teal'\n                            size='tiny'\n                        >\n                            <Icon name='user plus' />\n                            REGISTER\n                        </Button>\n                    </div>\n                    <div>\n                        <Label style={{ fontSize: '15px' }}>\n                            Back to Home\n                        </Label>\n                        <Icon name='hand point right' size='large' />\n                        <Label style={{ fontSize: '10px' }} />\n                   